# set custom git prompt

# These plugins must be loaded manually
# . $ZSH_CUSTOM/plugins/tab-completion/tab-completion.sh
. $ZSH_CUSTOM/plugins/zsh-git-prompt/zshrc.sh
set ZSH_THEME_GIT_PROMPT_CACHE

#source ~/.dotfiles/zsh-trprompt/trprompt.plugin.zsh
ZSH_AUTOSUGGEST_HIGHLIGHT_STYLE='fg=6'
